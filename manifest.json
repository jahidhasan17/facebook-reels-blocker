{
  "manifest_version": 3,
  "name": "Facebook Reels Blocker",
  "version": "1.0",
  "description": "Hides Facebook Reels from your feed",
  "content_scripts": [
    {
      "matches": ["*://*.facebook.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": ["*://*.facebook.com/*"]
}